---
import {Icon} from 'astro-icon'
import {
  AnnouncementBar,
  Button,
  CustomerQuoteSection,
  StickyTextImageSection,
  CtaCardSection
} from '@littlesticks/odyssey-theme-components';
import PhotoGridSection from '../../components/sections/PhotoGridSection.astro'
import Layout from '../layouts/Page.astro';
import HomeHeroSection from '../components/sections/heros/HomeHeroSection.astro';

const seo = {
	title: 'Popstar Photo Booths | Photo Booth Rental in Raleigh, NC',
	description: 'Photo booth rental in Raleigh',
};
---

<AnnouncementBar text="Did you love working with us? Leave a Google review and support a small business!" href="https://g.page/r/CczYNMDcHxyIEBM/review" slot="announcement-bar"/>
<Layout seo={seo} >
	<HomeHeroSection />
	<CustomerQuoteSection
		quoteText="So much fun to have this easy to use photo booth at our family reunion! We have tons of memories to share with these photos!"
		customerName="Laura Hendrix McKillop"
	/>
  <StickyTextImageSection>
    <div slot="text-container">
      <h2>The Popstar Booth</h2>
      <h3>Features</h3>
      <p>
        <ul>
          <li>Photos, videos, gifs, boomerangs</li>
          <li>Instagram style filters</li>
          <li>Text, email, air drop and QR code sharing</li>
          <li>Real time live gallery of all your event photos</li>
		  <li>Custom photo templates</li>
		  <li>Custom tap to start screen</li>
      </ul>
      </p>
      <h3>Perfect For</h3>
      <p>
        <ul>
          <li>Concerts</li>
          <li>Birthday Parties</li>
          <li>Holiday Parties</li>
          <li>Brand Launches</li>
          <li>Weddings</li>
      </ul>
      </p>
      <Button href="https://popstar-photo-booths.checkcherry.com/reservation/set_event?event_type_id=84825" slot="column-1-cta">Check Date</Button>
      
    </div>
     <picture slot="image-container">
      <img
        src="/assets/images/home/popstar-booth.jpg"
        alt="Popstar Photo Booths"
        loading="lazy"
      />
    </picture>
  </StickyTextImageSection>
  <h3 class="centered">Gallery</h3>
  <PhotoGridSection>
		<div slot="column-1-body">
		  <img src="/assets/images/home/image4.jpg" alt="Form" />
		</div>
		<div slot="column-2-body">
		  <img src="/assets/images/home/image3.jpg" alt="Select Date" />
    </div>
		<div slot="column-3-body">
		  <img src="/assets/images/home/image5.jpg" alt="Add Ons" />
		</div>
	</PhotoGridSection>
  <PhotoGridSection>
		<div slot="column-1-body">
		  <img src="/assets/images/home/image6.jpg" alt="Back Drops" />
		</div>
		<div slot="column-2-body">
		  <img src="/assets/images/home/image7.jpg" alt="Venue" />
    </div>
		<div slot="column-3-body">
		  <img src="/assets/images/home/image8.jpg" alt="Review" />
		</div>
	</PhotoGridSection>
	<CustomerQuoteSection
  quoteText="Dan and his team are amazing at ensuring your guests have fun creating memories with his various photobooths! You will not be disappointed!"
  customerName="Micah Brown - Amore Planning Co."
/>
  <StickyTextImageSection>
    <div slot="text-container">
      <h2>The Rockstar Booth</h2>
      <h3>Features</h3>
      <p>
        <ul>
		  <li>Professional quality lighting</li>
		  <li>Unlimited professional photos</li>
          <li>Glam Mode option</li>
          <li>Unlimited prints</li>
		  <li>Photos, videos, gifs, boomerangs</li>
          <li>Instagram style filters</li>
          <li>Text, email, air drop and QR code sharing</li>
          <li>Real time live gallery of all your event photos</li>
		  <li>Custom photo templates</li>
		  <li>Custom tap to start screen</li>
      </ul>
      </p>
      <h3>Pefect For</h3>
      <p>
        <ul>
		  <li>Upscale Weddings</li>
		  <li>Red Carpet Events</li>
          <li>High Budget Parties</li>
      </ul>
      </p>
      <Button href="https://popstar-photo-booths.checkcherry.com/reservation/set_event?event_type_id=84825" slot="column-1-cta">Check Date</Button>
      
    </div>
     <picture slot="image-container">
      <img
        src="/assets/images/home/rockstar-booth.jpg"
        alt="Popstar Photo Booths"
        loading="lazy"
      />
    </picture>
  </StickyTextImageSection>
  <h3 class="centered">Gallery</h3>
  <PhotoGridSection>
		<div slot="column-1-body">
		  <img src="/assets/images/home/rock-1.jpeg" alt="Form" />
		</div>
		<div slot="column-2-body">
		  <img src="/assets/images/home/rock-2.jpeg" alt="Select Date" />
    </div>
		<div slot="column-3-body">
		  <img src="/assets/images/home/rock-3.jpeg" alt="Add Ons" />
		</div>
	</PhotoGridSection>
  <PhotoGridSection>
		<div slot="column-1-body">
		  <img src="/assets/images/home/rock-4.jpeg" alt="Back Drops" />
		</div>
		<div slot="column-2-body">
		  <img src="/assets/images/home/rock-5.jpeg" alt="Venue" />
        </div>
		<div slot="column-3-body">
		  <img src="/assets/images/home/rock-6.jpeg" alt="Review" />
		</div>
	</PhotoGridSection>
	<PhotoGridSection>
		<div slot="column-1-body">
		  <img src="/assets/images/home/rock-7.jpeg" alt="Back Drops" />
		</div>
		<div slot="column-2-body">
		  <img src="/assets/images/home/rock-8.jpeg" alt="Venue" />
        </div>
		<div slot="column-3-body">
		  <img src="/assets/images/home/rock-9.jpeg" alt="Review" />
		</div>
	</PhotoGridSection>
  <CustomerQuoteSection
  quoteText="Great quality pictures to capture amazing memories! 5 stars for sure!!"
  customerName="Danielle Vitti"
/>
  <CtaCardSection>
  <h2>Ready to get started?</h2>
  <p>Let's do it!</p>
  <Button href="https://popstar-photo-booths.checkcherry.com/reservation/set_event?event_type_id=84825" customIcon>
    Get Quote
    <Fragment slot="icon">
      <Icon name="ic:baseline-chevron-right" />
    </Fragment>
  </Button>
</CtaCardSection>

</Layout>

<style>
.centered {
  text-align: center;
}
img {
  border-radius: var(--theme-shape-radius);
}
</style>